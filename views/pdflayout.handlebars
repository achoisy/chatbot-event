{{#each print }}
    <div class="cover" style="page-break-after:always;">
        <img id="cover-img" class="responsive-img" src="{{this.eventId}}/{{this.cover_public_id}}.jpg">
    </div>
    {{#each this.photoList }}
        <div class="row">
            <div class="col s12" style="page-break-after:always;">
                <div class="card">
                    <div class="card-image">
                        <img src="{{../eventId}}/{{this.public_id}}.jpg">
                        <span class="card-title">Card Title</span>
                    </div>
                    <div class="card-content">
                        <p>{{this.message}}</p>
                    </div>
                </div>
            </div>
        </div>
    {{/each}}
{{/each}}
