<!DOCTYPE html>
<html>
<head>
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="css/materialize.min.css" media="all" />
    <link type="text/css" rel="stylesheet" href="css/custom.css" media="all" />
    <meta charset="utf-8">
    <title>MonMagazine.fr</title>

</head>
<body>

  {{#each print }}
    <div class="cover" style="page-break-after:always;">
      <img id="cover-img" class="responsive-img" src="{{this.eventId}}/{{this.cover_public_id}}.jpg">
    </div>
      {{#each this.photoList }}
      <div class="row">
          <div class="col s12" style="page-break-after:always;">
            <div class="card">
              <div class="card-image">
                <img src="{{../eventId}}/{{this.public_id}}.jpg">
                <span class="card-title">Card Title</span>
              </div>
              <div class="card-content">
                <p>{{this.message}}</p>
              </div>
            </div>
          </div>
        </div>
      {{/each}}
  {{/each}}

</body>
</html>
